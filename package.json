{
  "name": "node-api-seed",
  "title": "Node Api Seed",
  "version": "0.0.0",
  "deploymentDate": "2016-10-22T14:48:26.240Z",
  "description": "The seed for pretty much any api I write in node.js",
  "main": "index.js",
  "scripts": {
    "start": "nodejs-dashboard node index.js",
    "watch": "nodemon .",
    "test": "node_modules/.bin/istanbul --include-all-sources cover node_modules/mocha/bin/_mocha test/**/*.js",
    "mocha": "node_modules/.bin/_mocha test/**/*.js",
    "lint": "eslint \"modules/**/*.js\" \"src/**/*.js\" \"test/**/*.js\" index.js --fix",
    "lint-ci": "eslint \"modules/**/*.js\" \"src/**/*.js\" \"test/**/*.js\" index.js",
    "report": "plato -r -d report -e .eslintrc src",
    "check-dependencies": "npm-check -Eu",
    "setDeploymentDate": "node ./utilities/set-deployment-date/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eXigentCoder/node-api-seed.git"
  },
  "keywords": [
    "node",
    "seed",
    "api",
    "express"
  ],
  "author": "Ryan Kotzen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eXigentCoder/node-api-seed/issues"
  },
  "homepage": "https://github.com/eXigentCoder/node-api-seed#readme",
  "dependencies": {
    "ajv": "4.8.2",
    "async": "2.1.2",
    "body-parser": "1.15.2",
    "boom": "4.2.0",
    "cors": "2.8.1",
    "express": "4.14.0",
    "get-parameter-names": "0.3.0",
    "json-schema-faker": "0.3.6",
    "json-schema-filter": "0.1.5",
    "json-schema-ref-parser": "3.1.2",
    "lodash": "4.16.4",
    "moment": "2.15.1",
    "morgan": "1.7.0",
    "nconf": "0.8.4",
    "node-glob-loader": "0.0.15",
    "nodejs-dashboard": "0.1.2",
    "pluralize": "3.0.0",
    "request-id": "0.11.0",
    "swagger-spec-express": "1.0.7",
    "swagger-ui": "2.2.6",
    "winston": "2.2.0",
    "winston-daily-rotate-file": "1.3.1",
    "winston-loggly": "1.3.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
